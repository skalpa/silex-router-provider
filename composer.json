{
    "name": "silicates/silex-router-provider",
    "type": "library",
    "description": "Full Symfony Router support (caching, yaml, xml, annotations) for Silex 2.x",
    "keywords": ["silex", "provider", "symfony", "routing", "router", "yaml", "annotations"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Pascal Luna",
            "email": "skalpa@zetareticuli.org"
        }
    ],
    "require": {
        "php": ">=5.6",
        "silex/silex": "^2.0",
        "symfony/config": "^2.8|^3.0",
        "symfony/routing": "^2.8|^3.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.4",
        "doctrine/cache": "^1.6",
        "sensio/framework-extra-bundle": "^4.0@dev",
        "symfony/yaml": "^2.8|^3.0",
        "symfony/phpunit-bridge": "^3.2"
    },
    "suggest": {
        "symfony/yaml": "To load routes from Yaml files"
    },
    "autoload": {
        "psr-4": {
            "Skalpa\\Silex\\Symfony\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skalpa\\Silex\\Symfony\\Routing\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
